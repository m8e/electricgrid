{
    "name": "tomshaw/electricgrid",
    "description": "A lightning fast Livewire data table designed to be easy to use and theme.",
    "keywords": [
        "laravel",
        "livewire",
        "datagrid",
        "datatable",
        "grid",
        "table"
    ],
    "homepage": "https://github.com/tomshaw/electricgrid",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Shaw",
            "email": "tomshaw@tomshaw.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "dompdf/dompdf": "^2.0",
        "illuminate/console": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "require-dev": {
        "larastan/larastan": "^2.8",
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^7.8.1",
        "orchestra/testbench": "^8.9.1",
        "pestphp/pest": "^2.16",
        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "TomShaw\\ElectricGrid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomShaw\\ElectricGrid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse --ansi --memory-limit=-1",
        "baseline": "vendor/bin/phpstan analyse --ansi --memory-limit=-1 --generate-baseline",
        "format": "vendor/bin/pint --config pint.json",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomShaw\\ElectricGrid\\Providers\\ElectricGridServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
